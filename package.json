{
	"name": "efficy-u-rpc-api",
	"description": "The Efficy U - RPC API is developed for server-side usage in a Node.js environment (e.g. for integrations) and also bundled for usage inside an Efficy browser session for client-side JSON RPC requests. The API is highly similar to efficy-enterprise-api",
	"main": "./dist/efficy-u-rpc-api-bundle.js",
	"scripts": {
		"build": "npm run build-all",
		"build:clean": "rimraf dist && rimraf docs",
		"build:ts": "tsc",
		"build:js": "rollup -c rollup.config.js",
		"build:dts": "rollup -c rollup.config.dts.js",
		"build:docs": "npx typedoc src/index.ts",
		"watch": "tsc -w",
		"test": "ava",
		"build-all": "npm run build:clean && npm run build:ts && npm run build:js && npm run build:dts && npm run build:docs",
		"pack": "npm pack --pack-destination pack/"
	},
	"version": "2.0.2",
	"homepage": "https://github.com/Pauwris/efficy-u-rpc-api#readme",
	"keywords": [
		"Efficy",
		"CRM",
		"U",
		"JSON RPC",
		"Node.js"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Pauwris/project/issues"
	},
	"repository": {
		"type": "git",
		"repository": "https://github.com/Pauwris/efficy-u-rpc-api.git"
	},
	"author": {
		"name": "Kristof Pauwels"
	},
	"dependencies": {
		"tslib": "^2.6.2"
	},
	"devDependencies": {
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/cookie": "^0.6.0",
		"@types/node": "^20.10.4",
		"@typescript-eslint/eslint-plugin": "^7.3.1",
		"@typescript-eslint/parser": "^7.3.1",
		"ava": "^6.1.2",
		"dotenv": "^16.4.5",
		"eslint": "^8.57.0",
		"eslint-plugin-promise": "^6.1.1",
		"ink-docstrap": "^1.3.2",
		"rollup-plugin-dts": "^6.1.0",
		"typedoc": "^0.25.13",
		"typescript": "^5.4.3",
		"typescript-eslint": "^7.3.1"
	},
	"type": "module",
	"files": [
		"dist/efficy-u-rpc-api-bundle.js",
		"dist/efficy-u-rpc-api-bundle.d.ts"
	]
}
